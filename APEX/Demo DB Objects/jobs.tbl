CREATE TABLE HR.JOBS 
    ( 
     JOB_ID VARCHAR2 (10 BYTE)  NOT NULL , 
     JOB_TITLE VARCHAR2 (35 BYTE)  NOT NULL , 
     MIN_SALARY NUMBER (6) , 
     MAX_SALARY NUMBER (6) 
    )  
;


CREATE UNIQUE INDEX HR.JOB_ID_PKX ON HR.JOBS 
    ( 
     JOB_ID ASC 
    ) 
;

ALTER TABLE HR.JOBS 
    ADD CONSTRAINT JOB_ID_PK PRIMARY KEY ( JOB_ID ) ;

